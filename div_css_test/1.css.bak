body {
	background:#2286c6;
	margin:0px;
	padding:0px;
	font-size:12px;
	font-family:Arial;
}

#container {
	margin:0px auto;
	width:800px;
	/*height:770px;*/
	text-align:left;
	background:#ffffff;
}

#banner {
	margin:0px;
	padding:0px;
	background:#789;
	height:150px;
	width:100%;
}

#globllink {
	margin:0px;
	padding:0px;
	
}
#globllink ul {
	list-style-type:none;
	padding:0px;
	margin:0px;
}
#globllink li {
	float:left;
	text-align:center;
	width:80px;
}
#globllink a {
	display:block;
	padding:9px 6px 10px 6px;
	margin:0px;
	background:url("image/1.jpg") no-repeat;
	
}
#globllink a:link,#globllink a:visited {
	color:#004a87;	
	text-decoration:underline;
}
#globllink a:hover {
	color:#ffffff;	
	text-decoration:none;
	background:url("image/2.jpg") no-repeat;
}

#left {
	width:200px;
	/*height:560px;*/
	background:#5ea6eb;
	margin:0px;
	padding:0px 0px 0px 0px;
	float:left;
	
}
#weather {
	background:url("image/weather.jpg") no-repeat;
	margin:-10px 5px;
	background-color:#5ea6eb;
	
}
#weather h3 {
	padding:6px 0px 0px 70px;
	font-size:12px;
	color:#ffffff;
}
#weather ul {
	padding:14px;
	margin:-10px 5px 0px 5px;
}
#today h3 {
	padding:6px 0px 5px 70px;
	font-size:12px;
	color:black;
	font-weight:bold;
	background:#bbddff;
}
#today ul {
	list-style-type:none;
	padding:3px 3px 0px 3px;
	margin:10px 0px 3px 0px;
	text-align:center;
	
}
#today li {
	margin:3px auto;
}
#today ul img {
	border:1px solid #ffffff;
}
#today ul li a:link,#today ul li a:visited {
	color:black;
	text-decoration:none;
}
#today ul li a:hover {
	color:red;
	text-decoration:underline;
}

#middle {
	width:400px;
	/*height:560px;*/
	background:white;
	margin:0px;
	padding:0px 0px 0px 0px;
	float:left;
}
#middle div {
	margin:5px 5px 0px 5px;
}

#beauty {
	margin:15px 0px 10px 0px;
	padding:0px 0px 10px 0px;
}
#middle h3 {
	margin:0px;
	padding:8px;
}
#route {
	clear:left;
}
#route h3 {
	padding:12px 0px 0px 9px;
}
#beauty ul{
	list-style-type:none;
	margin:8px 1px 0px 1px;
	padding:4px 2px 0px 4px;
}
#beauty ul li {
	float:left;
	margin:0px 1px;
}
#route ul li {
	padding:5px 5px 5px 12px;
}
#route ul li a:link,#route ul li a:visited {
	color:black;
	text-decoration:none;
}
#route ul li a:hover {
	color:red;
	text-decoration:underline;
}
#route ul {
	background:#5ea6eb;
	border:1px solid blue;
	margin:18px 0px;
	padding:0px 5px 0px 20px;
}
#right {
	width:200px;
	/*height:560px;*/
	background:white;
	margin:0px;
	padding:0px 0px 0px 0px;
	float:left;
}
#right div {
	background:#5ea6eb;
	margin:0px 5px;
}
#right div h3 {
	font-size:12px;
	padding:4px 0px 2px 15px;
	color:#003973;
	margin:0px 0px 5px 0px;
	background:#bbddff;
}
#map p {
	text-align:center;
	margin:0px 4px 0px 4px;
	padding:2px 0px 5px 0px;
}
#map p img {
	border:1px solid white;
}
#food ul,#life ul {
	padding:0px 0px 10px 10px;
	margin:5px 10px 0px 5px;
}
#food ul li,#life ul li {
	padding:4px 0px 4px 0px;
	border-bottom:1px dashed white;
}
#food ul li a,#life ul li a {
	color:black;
}
#food ul li a:link,#life ul li a:link,#food ul li a:visited,#life ul li a:visited {
	color:black;
	text-decoration:none;
}
#food ul li a:hover,#life ul li a:hover {
	color:red;
	text-decoration:underline;
}
#footer {
	width:100%;
	/*height:30px;*/
	background:#ffffff;
	margin:0px;
	padding:1px 0px;
	clear:both;
}
#footer p {
	background:#5ea6eb;
	padding: 0px;
	margin:4px 5px 4px 5px;
	text-align:center;
}